<%= simple_form_for monster do |f| %>
<%= f.input :name %>
<%= f.input :species %>
<%= f.input :ability %>
<%= f.input :address %>
<%= f.input :price %>
<%= f.input :photo, as: :file %>
<%= f.submit %>
<% end %>
<%= link_to 'Go Back', :back, class: 'btn btn-secondary mt-2' %>
